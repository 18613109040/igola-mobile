    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "beidou-cli": "^1.1.0",
    "beidou-core": "^1.1.0",
    "classnames": "^2.2.5",
    "dotenv": "^6.2.0",
    "dva": "^2.4.0",
    "dva-loading": "2.0.5",
    "egg-alinode": "^2.0.1",
    "egg-cors": "2.1.0",
    "hash.js": "^1.1.5",

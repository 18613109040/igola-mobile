{
  "name": "cms",
  "version": "1.0.0",
  "description": "cms beidou dva",
  "scripts": {
    "start": "ENABLE_NODE_LOG=YES beidou start --daemon --ignore-stderr --title=egg-server-cms",
    "debug": "beidou debug",
    "stop": "beidou stop --title=egg-server-cms",
    "dev": "beidou dev",
    "prod": "beidou start --env=prod --workers=1",
    "sit": "beidou start --env=sit --workers=1",
    "uat": "beidou start --env=uat --workers=1",
    "build": "npm run clean && beidou build",
    "clean": "rimraf ./build/*",
    "analyzer": "beidou build --env=test --report",
    "build:node": "beidou build --target=node"
  },
  "license": "MIT",
